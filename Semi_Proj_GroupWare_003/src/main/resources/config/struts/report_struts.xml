<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<!-- namespace 경로 /액션의이름:name URL= /actionname -->
	<!-- tiles 이름 경로 -->
	<package name="report_struts" namespace="/" extends="default">
		
		<!-- 
		  report_add
		  report_detail
	      report_list
		 -->
		<action name="report_list">
			<result type="tiles">report_list</result>
		</action>
		<action name="report_add">
			<result type="tiles">report_add</result>
		</action>
		<action name="report_detail">
			<result type="tiles">report_detail</result>
		</action>
	</package>
</struts>
